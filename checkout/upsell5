  <script>
    document.addEventListener('DOMContentLoaded', function () {
      // Rotator animation
      const rotator = document.querySelector('.bg-rotator');
      if (rotator) {
        let currentRotation = 21;
        let direction = -1;
        const rotationSpeed = 0.2;
        const maxRotation = 21;
        const minRotation = -160;
        function animateRotator() {
          currentRotation += direction * rotationSpeed;
          if (currentRotation <= minRotation) {
            currentRotation = minRotation;
            direction = 1;
          } else if (currentRotation >= maxRotation) {
            currentRotation = maxRotation;
            direction = -1;
          }
          rotator.style.transform = `rotateZ(${currentRotation}deg)`;
          requestAnimationFrame(animateRotator);
        }
        animateRotator();
      }
    });
  </script>
  <script>
    (function () {
      function readCookie(name) {
        var m = document.cookie.match(new RegExp('(?:^|; )' + name + '=([^;]*)'));
        return m ? decodeURIComponent(m[1]) : undefined;
      }
      function setCookie(name, value, maxAgeSec) {
        var domain = location.hostname.replace(/^www\./, '');
        document.cookie = name + '=' + encodeURIComponent(value) +
          ';path=/;domain=' + domain + ';samesite=lax' +
          (maxAgeSec ? ';max-age=' + maxAgeSec : '');
      }

      // If URL has fbclid, (re)create _fbc in the correct format (SECONDS)
      var p = new URLSearchParams(location.search);
      var fbclid = p.get('fbclid');
      if (fbclid) {
        var fbc = 'fb.1.' + Math.floor(Date.now() / 1000) + '.' + fbclid;
        setCookie('_fbc', fbc, 60 * 60 * 24 * 90);
      }

      // If _fbc exists but its timestamp is in ms, fix it to seconds
      var fbcCookie = readCookie('_fbc');
      if (fbcCookie) {
        var parts = fbcCookie.split('.');
        if (parts.length >= 4) {
          var ts = parts[2];
          if (/^\d{13}$/.test(ts)) { // ms -> sec
            parts[2] = String(Math.floor(Number(ts) / 1000));
            setCookie('_fbc', parts.slice(0, 4).join('.'), 60 * 60 * 24 * 90);
          }
        }
      }

      // Expose helper globally
      window.__getFBC = function () { return readCookie('_fbc'); };
    })();
  </script>
