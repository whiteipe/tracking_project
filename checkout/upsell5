    )
    function setUpsell(id) {
      console.log(id);
      const btnAdd = document.querySelectorAll('.yes-button');
      btnAdd.forEach((btn) => {
        btn.setAttribute('upsell', id);
      }
      );
    }
    function dataLayerPush(event) {
      // get fbc & fbp

      const fbpCookie = document.cookie.split('; ').find(cookie => cookie.startsWith('_fbp='));
      const fbpValue = fbpCookie ? fbpCookie.split('=')[1] : null;
      const validFormatRegex = /^fb\.1\.\d+\.\d+$/;
      const isFbpValid = validFormatRegex.test(fbpValue);

      // Check if the values are correctly formatted
      console.log(event);
      const data = JSON.parse(sessionStorage.getItem("totals"));
      window.dataLayer = window.dataLayer || [];
      let pbParams = "";
      event.includes('Sale') ? pbParams = true : pbParams = false;
      const convertionLabels = {
        'Downsell3View': '',
        'Downsell3Sale': '',
      }
      dataLayer.push({
        'event': 'champEvent',
        'champEventLabel': event,
        'champEventId': localStorage.getItem('unique_event_id_facebook'),
        'champProdName': 'Pest Defense',
        'champTotal': currentSelectionPrice,
        'champRedTrackSum': pbParams,
        "champEmail": data.emailAddress,
        "champPhone": data.phoneNumber,
        "champFirstName": data.firstName,
        "champLastName": data.lastName,
        "champCity": data.city,
        "champRegion": data.state,
        "champPostalCode": data.postalCode,
        "champCountry": data.country,
        "champProducts": JSON.stringify([{
          "id": currentSelectionId,
          "quantity": 1
        }
        ]),
        "fbclid": window.__getFBC() || null,
        "fbplid": isFbpValid ? fbpValue : null,
        'champConversionLabel': convertionLabels[event],
        'champFacebookContentIDProduct': "VAMOOSE"
      }
      );
    }
