    const dataUri = sessionStorage.getItem("requestUri");
    const data = JSON.parse(sessionStorage.getItem("orderData"));
    let salesUrl = new URL(dataUri);
    const clickid = salesUrl.searchParams.get('clickid');
    setCookie("rtkclickid-store", `${clickid}`, 1);
    function setCookie(name, value, daysToLive) {
      const date = new Date();
      date.setTime(date.getTime() + daysToLive * 24 * 60 * 60 * 1000);
      date.toUTCString();
      let expires = "expires=" + date.toUTCString();
      document.cookie = `${name}=${value}; ${expires}; path=/`;
    }
  </script>
