    let currentSelectionId = 67370
    let currentSelectionPrice = 45.00
    window.addEventListener('DOMContentLoaded', () => {
      dataLayerPush("Downsell3View");
      setUpsell(currentSelectionId)
      const btnAccept = document.querySelectorAll('.yes-button');
      btnAccept.forEach(item => {
