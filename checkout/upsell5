  <script>
    // Google Enhanced Conversions
    function hashData(data) {
      const hashedResult = CryptoJS.SHA256(data).toString(CryptoJS.enc.Hex);
      return hashedResult;
    }
    function formatToE164(number) {
      const cleanedNumber = number.replace(/\D/g, '');
      return `+1${cleanedNumber}`;
    }
    var emailEnhancedConversions = data.emailAddress.trim().toLowerCase()
