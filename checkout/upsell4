    setTimeout("preventBack()", 0);
    window.onunload = function () {
      null
    };
  </script>
  <script>
    const dataUri = sessionStorage.getItem("requestUri");
    const data = JSON.parse(sessionStorage.getItem("orderData"));
    let salesUrl = new URL(dataUri);
    const clickid = salesUrl.searchParams.get('clickid');
    setCookie("rtkclickid-store", `${clickid}`, 1);
    function setCookie(name, value, daysToLive) {
      const date = new Date();
      date.setTime(date.getTime() + daysToLive * 24 * 60 * 60 * 1000);
      date.toUTCString();
      let expires = "expires=" + date.toUTCString();
      document.cookie = `${name}=${value}; ${expires}; path=/`;
    }
  </script>
  <script>
    let currentSelectionId = 67371
    let currentSelectionPrice = 140.00
    window.addEventListener('DOMContentLoaded', () => {
      dataLayerPush("Upsell3View");
      setUpsell(currentSelectionId)
      const btnAccept = document.querySelectorAll('.yes-button');
      btnAccept.forEach(item => {
        item.addEventListener('click', () => {
          dataLayerPush("Upsell3Sale");
        }
        )
      }
      )
    }
    )
    function setUpsell(id) {
      console.log(id);
      const btnAdd = document.querySelectorAll('.yes-button');
      btnAdd.forEach((btn) => {
        btn.setAttribute('upsell', id);
      }
      );
    }
    function dataLayerPush(event) {
      // get fbc & fbp

      const fbpCookie = document.cookie.split('; ').find(cookie => cookie.startsWith('_fbp='));
      const fbpValue = fbpCookie ? fbpCookie.split('=')[1] : null;
      const validFormatRegex = /^fb\.1\.\d+\.\d+$/;
      const isFbpValid = validFormatRegex.test(fbpValue);

      // Check if the values are correctly formatted
      console.log(event);
      const data = JSON.parse(sessionStorage.getItem("totals"));
      window.dataLayer = window.dataLayer || [];
      let pbParams = "";
      event.includes('Sale') ? pbParams = true : pbParams = false;
      const convertionLabels = {
        'Upsell3View': '',
        'Upsell3Sale': '',
      }
      dataLayer.push({
        'event': 'champEvent',
        'champEventLabel': event,
        'champEventId': localStorage.getItem('unique_event_id_facebook'),
        'champProdName': 'Pest Defence',
        'champTotal': currentSelectionPrice,
        'champRedTrackSum': pbParams,
        "champEmail": data.emailAddress,
        "champPhone": data.phoneNumber,
        "champFirstName": data.firstName,
        "champLastName": data.lastName,
        "champCity": data.city,
        "champRegion": data.state,
        "champPostalCode": data.postalCode,
        "champCountry": data.country,
        "champProducts": JSON.stringify([{
          "id": currentSelectionId,
          "quantity": 1
        }
        ]),
        "fbclid": window.__getFBC() || null,
        "fbplid": isFbpValid ? fbpValue : null,
        'champConversionLabel': convertionLabels[event],
        'champFacebookContentIDProduct': "VAMOOSE"
      }
      );
    }
  </script><!-- Hash Data -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/crypto-js/3.1.9-1/crypto-js.js"></script>
  <script>
    // Google Enhanced Conversions
    function hashData(data) {
      const hashedResult = CryptoJS.SHA256(data).toString(CryptoJS.enc.Hex);
      return hashedResult;
    }
    function formatToE164(number) {
      const cleanedNumber = number.replace(/\D/g, '');
      return `+1${cleanedNumber}`;
    }
    var emailEnhancedConversions = data.emailAddress.trim().toLowerCase()
    var phonenumberEnhancedConversions = formatToE164(data.phoneNumber.trim())
    var firstnameEnhancedConversions = data.firstName.trim().toLowerCase()
    var lastnameEnhancedConversions = data.lastName.trim().toLowerCase()
    var streetEnhancedConversions = data.address1.trim().toLowerCase()
    var cityEnhancedConversions = data.city.trim().toLowerCase()
    var regionEnhancedConversions = data.state.trim().toLowerCase()
    var postalcodeEnhancedConversions = data.postalCode.trim().toLowerCase()
    var countryEnhancedConversions = data.country.trim().toLowerCase()
    console.log(emailEnhancedConversions, firstnameEnhancedConversions, lastnameEnhancedConversions, phonenumberEnhancedConversions, streetEnhancedConversions, cityEnhancedConversions, countryEnhancedConversions, regionEnhancedConversions, postalcodeEnhancedConversions);
  </script>
  <script>var items = document.querySelectorAll('#iwjle');
    for (var i = 0, len = items.length; i < len; i++) {
      (function () {

        const el = this;
        el.addEventListener('mouseover', eventHandler);
        const animateClasses = [
          'pulse',
          '',
          '',
        ]
        const animateHoverClasses = [
          '',
          '',
          '',
        ]
        const animateFilterClass = animateClasses.filter(data => data !== "");
        const animateHoverFilterClass = animateHoverClasses.filter(data => data !== "");
        function eventHandler(e) {
          $('#iwjle').removeClass(animateFilterClass.join(" "));
          $('#iwjle').css({ 'animation-delay': '' });
          $('#iwjle').addClass(['animated', animateHoverFilterClass.join(" ")]);
        };
        el.addEventListener('mouseleave', mouseLeaveHandler);
        function mouseLeaveHandler(e) {
          $('#iwjle').removeClass(animateHoverFilterClass.join(" "));
          $('#iwjle').addClass(animateFilterClass.join(" "));
          if (window.location.pathname !== "blank") {
            var minutesDelay = '0';
            var secondsDelay = '0';
            $('#iwjle').css({ 'animation-delay': minutesDelay > 0 ? (parseInt((minutesDelay * 60)) + parseInt(secondsDelay)) + "s" : secondsDelay !== undefined ? parseInt(secondsDelay) + "s" : "" });
          }
        };

      }.bind(items[i]))();
    }</script>
  <script id="scriptData">
    window.addEventListener('load', (function () {
      $("#loading").hide();
      $("body").removeClass("dom-pending");

      var lazyLoadThrottleTimeout;
      function recursiveOffsetParentTop(img) {
        if (img.offsetParent) {
          return img.offsetParent.offsetTop + recursiveOffsetParentTop(img.offsetParent);
        } else {
          return 0;
        }
      }
      function lazyLoad() {
        if (lazyLoadThrottleTimeout) {
          clearTimeout(lazyLoadThrottleTimeout);
        }
        lazyLoadThrottleTimeout = setTimeout(function () {
          const scrollTop = window.pageYOffset;
          const buffer = 500;
          var loaded = true;
          //Lazy loading the images
          var lazyLoad = [].slice.call(document.querySelectorAll(".fk-lazy"));
          lazyLoad.forEach(function (element) {
            if ($(element).is(':visible') && element.offsetTop + recursiveOffsetParentTop(element) < ((window.innerHeight + scrollTop) + buffer)) {
              if (element.dataset.src) {
                element.src = element.dataset.src;
              }
              element.classList.remove("fk-lazy");
            } else {
              loaded = false;
            }
          });
          if (loaded) {
            document.removeEventListener("scroll", lazyLoad);
            window.removeEventListener("resize", lazyLoad);
            window.removeEventListener("orientationChange", lazyLoad);
          }
        }, 20);
      }

      document.addEventListener("scroll", lazyLoad);
      window.addEventListener("resize", lazyLoad);
      window.addEventListener("orientationChange", lazyLoad);
      lazyLoad();
    }), false);
  </script>


  <div data-cc-link-attribution="true"><a href="https://checkoutchamp.com" title="Powered by CheckoutChamp"></a></div>
  <script>


    var dynamicCartRow = document.querySelector("tr[id='fk-dynamic-cart-row']");
    if (dynamicCartRow) {
      dynamicCartRow.style.display = "none";
    }
    function getAbsolutePath(url) {
      const parts = url.split('/');
      var size = 3;
      if (parts[2].indexOf("thisisatestsiteonly") !== -1) {
        size = 4;
      }
      return parts.slice(0, size).join("/");
    }
    var fkDynamicScript = document.createElement("script");
    fkDynamicScript.type = "text/javascript";
    fkDynamicScript.src = getAbsolutePath(window.location.href) + "/bugmdchampoffersop-vamoose-523-30-30-60-60-WOS-socialproof-ship/index.js?f=34d508";
    document.head.appendChild(fkDynamicScript);
  </script>


</body>

</html>